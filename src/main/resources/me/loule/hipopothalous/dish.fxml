<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.loule.hipopothalous.controller.Dish">
   <children>
      <VBox alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="25.0" prefWidth="600.0">
               <children>
                  <Label text="Liste des plats" />
                  <Button fx:id="addDishesButton" mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
            <ScrollPane fx:id="dishScrollPane" prefHeight="200.0" prefWidth="200.0">
              <content>
                <AnchorPane fx:id="dishAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="200.0">
                     <children>
                        <VBox fx:id="listVBox" prefHeight="200.0" prefWidth="600.0" />
                     </children>
                </AnchorPane>
              </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
